              <![CDATA[
                  div {
                    position: relative;
                  }
                  img {
                    width: 100%;
                    object-fit: scale-down;
                  }
                  input[type="button"] {
                    -moz-appearance: none;
                    appearance: none;
                    background-color: #000;
                    background-image: url('data:image/svg+xml,%3Csvg width="9" height="11" viewBox="0 0 9 11" fill="none" xmlns="http://www.w3.org/2000/svg">%3Cpath d="M2.71607 0.35558C2.82455 0.136607 3.04754 0 3.29063 0H5.70938C5.95246 0 6.17545 0.136607 6.28393 0.35558L6.42857 0.642857H8.35714C8.71272 0.642857 9 0.930134 9 1.28571C9 1.64129 8.71272 1.92857 8.35714 1.92857H0.642857C0.287277 1.92857 0 1.64129 0 1.28571C0 0.930134 0.287277 0.642857 0.642857 0.642857H2.57143L2.71607 0.35558ZM0.642857 2.57143H8.35714V9C8.35714 9.70915 7.78058 10.2857 7.07143 10.2857H1.92857C1.21942 10.2857 0.642857 9.70915 0.642857 9V2.57143ZM2.57143 3.85714C2.39464 3.85714 2.25 4.00179 2.25 4.17857V8.67857C2.25 8.85536 2.39464 9 2.57143 9C2.74821 9 2.89286 8.85536 2.89286 8.67857V4.17857C2.89286 4.00179 2.74821 3.85714 2.57143 3.85714ZM4.5 3.85714C4.32321 3.85714 4.17857 4.00179 4.17857 4.17857V8.67857C4.17857 8.85536 4.32321 9 4.5 9C4.67679 9 4.82143 8.85536 4.82143 8.67857V4.17857C4.82143 4.00179 4.67679 3.85714 4.5 3.85714ZM6.42857 3.85714C6.25179 3.85714 6.10714 4.00179 6.10714 4.17857V8.67857C6.10714 8.85536 6.25179 9 6.42857 9C6.60536 9 6.75 8.85536 6.75 8.67857V4.17857C6.75 4.00179 6.60536 3.85714 6.42857 3.85714Z" fill="white"/%3E%3C/svg%3E');
                    background-position: center;
                    background-repeat: no-repeat;
                    border-radius: 5px;
                    border: 0;
                    cursor: pointer;
                    height: 20px;
                    position: absolute;
                    right: 8px;
                    top: 8px;
                    width: 20px;
                  }
                  input[type="button"]:disabled {
                    cursor: auto;
                    opacity: 0.25;
                  }
              ]]>
            </bluel:custom-style>
            <div><!--
            --><img src="" alt=""/><!--
            --><input type="button" value=""/><!--
         --></div>
        </bluel:gallery-edit-item-template>
        <bluel:gallery-item-template>
            <bluel:custom-style>
              <![CDATA[
                figcaption, figure, img {
                    margin: 0;
                    padding: 0;
                    border: 0;
                    font-size: 100%;
                    font: inherit;
                    vertical-align: baseline;
                }
                figcaption, figure {
                    display: block;
                }
                img {
                    width: 100%;
                }
              ]]>
            </bluel:custom-style>
            <figure><!--
            --><img src="" alt=""/><!--
            --><figcaption></figcaption><!--
         --></figure>
        </bluel:gallery-item-template>
        <bluel:category-filter-template>
            <bluel:custom-style>
              <![CDATA[
                input[type="checkbox"] {
                    -moz-appearance: none;
                    appearance: none;
                    background-color: var(--cat-filter-background-color);
                    border-radius: 60px;
                    border: 2px solid var(--cat-filter-border-color);
                    cursor: pointer;
                    display: block;
                    font-family: 'Syne';
                    font-size: 1.1em;
                    font-weight: 700;
                    line-height: 1.0;
                    margin: 0;
                    overflow-x: hidden;
                    padding: 14px 30px;
                    text-overflow: ellipsis;
                    transition: background-color 0.3s;
                    white-space: nowrap;
                    width: 100%;
                }
                input[type="checkbox"]::after {
                    color: var(--cat-filter-font-color);
                    margin: 0;
                    padding: 0;
                    text-align: center;
                }
                input[type="checkbox"]:checked::after {
                    color: var(--cat-filter-checked-font-color);
                }
                input[type="checkbox"]:hover::after {
                    text-decoration: underline;
                }
                input[type="checkbox"]:checked {
                    background-color: var(--cat-filter-checked-background-color);
                }
                input[type="checkbox"]::after {
                    content: attr(data-category-name);
                }
              ]]>
            </bluel:custom-style>
            <input class="category-filter" type="checkbox" name=""/>
        </bluel:category-filter-template>
    </bluel:web-components>
</root>
